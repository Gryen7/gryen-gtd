{
    "name": "itargaryen/gryen-gtd",
    "keywords": ["gtd", "blog", "laravel"],
    "description": "A GTD App Based on Laravel v5.8.",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.0",
        "barryvdh/laravel-cors": "1.0.*",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "6.5.*",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "5.8.*",
        "predis/predis": "1.1.*",
        "spatie/laravel-feed": "^2.6",
        "spatie/laravel-sitemap": "5.3.*",
        "tymon/jwt-auth": "1.0.0-rc.5.1",
        "zgldh/qiniu-laravel-storage": "0.10.*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.6.*",
        "beyondcode/laravel-dump-server": "^1.0",
        "doctrine/dbal": "2.10.*",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate"
        ]
    }
}
